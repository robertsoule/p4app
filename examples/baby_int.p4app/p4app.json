{
    "program": "baby_int.p4",
    "language": "p4-16",
    "targets": {
	"multiswitch": {
	    "auto-control-plane": true,
	     "cli": true,
	    "links": [["h1", "s1"], ["s1", "s2"], ["s2", "h2", 50]],
	    "hosts": {
		"h1": {
		    "comment": "python receive.py --intf 'h1-eth0' --filter 'udp'",
		    "cmd": "python receive.py %port%",
		    "startup_sleep": 0.2,
		    "wait": false
		},
		"h2": {
		    "comment": "sudo python ./send.py --intf h2-eth0 --dst h1 --dport 8000",
		    "cmd": "python send.py h1 %port% %echo_msg%",
		    "wait": true

		}
	    },
	    "parameters": {
		"intf" : "eth0",
		"port": 8000,
		"echo_msg": "foobar"
	    }
	}
    }
}
